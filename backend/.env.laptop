# Laptop configuration - Use RunPod GPU via SSH tunnel
# This file is used when connecting to remote RunPod GPU server

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=[DB_NAME]
DB_USER=[DB_USER]
DB_PASSWORD=[DB_PASSWORD]

# LLM API 키
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# 임베딩 모델 설정
EMBEDDING_MODEL=nlpai-lab/KURE-v1
EMBEDDING_MODEL_NAME=nlpai-lab/KURE-v1
EMBEDDING_DIMENSION=1024

# Laptop: Remote embedding server on RunPod (via SSH tunnel on port 18001)
# SSH tunnel command: ssh -N -L 18001:127.0.0.1:8001 root@<runpod-pod-id>.pods.runpod.io
REMOTE_EMBED_URL=http://127.0.0.1:18001

# 애플리케이션 설정
APP_ENV=development
DEBUG=True
SECRET_KEY=
RETRIEVAL_MODE=hybrid

# CORS 설정
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# LangSmith 설정 (선택사항)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=ddoksori

# Hugging Face Token
HF_TOKEN=
